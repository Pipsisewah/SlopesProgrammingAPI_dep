  "scripts": {
    "tsc-build": "./node_modules/.bin/tsc",
    "build": "./node_modules/.bin/webpack-cli",
    "build-watch": "./node_modules/.bin/webpack-cli -w",
    "dev": "sam local start-api --env-vars ./infra/local-parameters.json",
    "test": "jest",
    "api": "sam local start-api --skip-pull-image --env-vars ./infra/local-parameters.json",
    "start-api": "npm run tsc-build && concurrently --kill-others --kill-others-on-fail \"npm run build:watch\" \"npm run api\""
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.3",
    "@types/validator": "^13.1.0",
    "@types/webpack": "^4.41.23",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "aws-sam-webpack-plugin": "^0.6.0",
    "concurrently": "^4.1.2",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "yaml-cfn": "^0.2.3"
  },
  "dependencies": {
    "aws-sdk": "^2.841.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.0.7",
    "webpack-node-externals": "^2.5.2"
  }
}
